<% productos.forEach(producto => { %>
    <div class='resultCard'>
        <div>
            <figure><img src="<%- producto.img %>" alt=""></figure>
        </div>
        <div>
            <div>
                <span>Nombre: </span><span><%- producto.name %></span>
            </div>
            <div>
                <span>Codigo de barras: </span><span><%- producto.id %></span>
            </div>
        </div>
    </div>
<% }) %>